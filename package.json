{
  "name": "code-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/auth": "^0.18.2",
    "@firebase/database": "^0.12.1",
    "@firebase/firestore": "^3.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "date-fns": "^2.25.0",
    "firebase": "^9.1.2",
    "formik": "^2.2.9",
    "history": "^5.0.1",
    "ky": "^0.28.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-router": "^6.0.0-beta.6",
    "react-router-dom": "^6.0.0-beta.6",
    "react-scripts": "4.0.3",
    "reactfire": "^4.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "typescript": "~4.4.3",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:fix": "npm run lint:es:fix && npm run lint:style:fix",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint' src/**/*.css'",
    "lint:style:fix": "stylelint --fix' src/**/*.css'",
    "emulate": "firebase emulators:start --import=./firebase --only firestore,auth,functions",
    "start:emulate": "firebase emulators:exec --import=./firebase --only firestore,auth,functions 'yarn start'",
    "test:ci": "react-scripts test --watchAll=false",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "firebase:export": "firebase emulators:export ./firebase",
    "firebase": "firebase",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/node-logger": "^6.3.10",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.3.10",
    "@types/babel__core": "^7.1.16",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "^2.4.1",
    "@types/react-helmet": "^6.1.3",
    "@types/react-is": "^17.0.2",
    "@types/stylelint": "^13.13.2",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__react": "^10.2.0",
    "@types/testing-library__user-event": "^4.2.0",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "8.1.0",
    "chromatic": "^5.10.2",
    "cypress": "^8.5.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "firebase-tools": "^9.20.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.1",
    "react-is": "^17.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.css": [
      "stylelint --fix"
    ],
    "functions/src/**/*.{js,ts}": [
      "cd functions/ && eslint --fix"
    ]
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
